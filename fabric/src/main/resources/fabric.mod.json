{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "repo": "https://github.com/lanAnderson/PocketGPS",
    "issues": "https://github.com/lanAnderson/PocketGPS/issues"
  },
  "license": "${license}",
  "icon": "${mod_id}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "club.iananderson.pocketgps.fabric.PocketGpsFabric"
    ],
    "client": [
      "club.iananderson.pocketgps.fabric.client.PocketGpsFabricClient"
    ]
  },
  "mixins": [
    "pocketgps.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${fabric_version_range}",
    "java": ">=${java_version}",
    "fabric-api": "*"
  }
}
